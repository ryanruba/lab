#include <iostream>
using namespace std;
const int size = 10;
int searchArray(double array[], double key) {
    for (int i = 0; i < size; i++)
        if (array[i] == key) return i;
    return -1;}
int main() {
    double array[size], key;
    cout << "Enter " << size << " values:\n";
    for (double &value : array) cin >> value;
    cout << "Search for: ";
    cin >> key;
    int result = searchArray(array, key);
    cout << (result != -1 ? "Found at index: " + to_string(result) : "Not Found") << endl;
    return 0;}
